{
  "name": "lindsay-wildlife-treasure-hunt-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@zxing/library": "^0.18.6",
    "dom-to-image": "^2.6.0",
    "enzyme": "^3.11.0",
    "jest": "26.6.0",
    "qrcode-reader": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-qr-reader": "^2.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-slider": "^1.3.1",
    "react-test-renderer": "^17.0.2",
    "react-zoom-pan-pinch": "^2.1.3",
    "web-vitals": "^1.0.1"
  },
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "unit-testing": "jest --verbose --rootDir=test/unit",
    "integration-testing": "jest --verbose --rootDir=test/integration",
    "eject": "react-scripts eject"
  },
  "homepage": "http://localhost/ui",
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "css"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest",
      "^.+\\.(css|less)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|svg|png|jpg|jpeg)$": "identity-obj-proxy"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@testing-library/jest-dom": "^5.16.1",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^3.0.0"
  }
}
